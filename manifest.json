{
  "manifest_version": 2,

  "name": "easy tabs",
  "description": "a simple extension to simplify tab management",
  "version": "1.0",

  "browser_action": {
    "default_icon": "easy-tabs.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}