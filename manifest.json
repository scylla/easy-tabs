{
  "manifest_version": 2,

  "name": "easy tabs",
  "description": "a simple extension to simplify tab management",
  "version": "1.0",

  "browser_action": {
    "default_icon": "easy-tabs.png",
    "default_popup": "popup.html",
    "default_title": "enter easy-tabs"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "background",
    "clipboardRead",
    "clipboardWrite",
    "debugger",
    "pageCapture",
    "tabCapture",
    "https://ajax.googleapis.com/"
  ]
}